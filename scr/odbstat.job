#!/bin/bash
#         
#    Extract statistics from acma-file 
#
set -x
#
ln -sf ccma_$DTG fort.10
# Generate namelist
#
cat  >odbstat.dat <<**EOF**
&odbstat
    lusage=.false.,
    lsatob=.false.,
    lsynop=.true.,
    lmetar=.false., 
    lship=.true.,
    ldribu=.true.,
    lpilot=.true.,
    lairep=.true.,
    ltemp=.true., 
    lpreplo=.true.,
/
**EOF**


# Run executable
$BINDIR/odbstat


